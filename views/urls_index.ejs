<!DOCTYPE html>
<html lang="en">
  <head>
    <title>TinyApp</title>
  </head>
  <body style="font-size:20px; padding:10px;">
  <header><% include partials/_header %></header>
    <ul>
    <% for(let key in urls) { %>
      <li>
      <%= key %> : <%= urls[key] %>
        <div>
          <form action="/urls/<%= key %>" method="GET" >
            <button style="float: left">Edit</button>
          </form>
          <form action="/urls/<%= key %>/delete" method="POST" >
            <button style="float: left">Delete</button>
          </form>
        </div>
      </li>
    <% } %>
    </ul>
    <div>
      <form action="/urls/new" method="GET" >
        <button>Create</button>
      </form>
    </div>
</body>
</html>

